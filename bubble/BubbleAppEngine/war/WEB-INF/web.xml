<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
  	<servlet-name>SignupServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.userinfo.SignupServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SignupServlet</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>UserInfoServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.userinfo.UserInfoServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserInfoServlet</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.userinfo.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>FriendServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.userinfo.FriendServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FriendServlet</servlet-name>
  	<url-pattern>/friend</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BubbleCreateServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.bubbledata.BubbleCreateServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleCreateServlet</servlet-name>
  	<url-pattern>/create</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>BubbleListServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.bubbledata.BubbleListServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleListServlet</servlet-name>
  	<url-pattern>/list</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>FavoriteServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.bubbledata.FavoriteServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FavoriteServlet</servlet-name>
  	<url-pattern>/favorite</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BubbleDetailServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.bubbledata.BubbleDetailServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleDetailServlet</servlet-name>
  	<url-pattern>/detail</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BubbleCommentServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.comment.BubbleCommentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleCommentServlet</servlet-name>
  	<url-pattern>/comment</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BubbleTagServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.tag.BubbleTagServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleTagServlet</servlet-name>
  	<url-pattern>/tag</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>TagSearchServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.tag.TagSearchServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>TagSearchServlet</servlet-name>
  	<url-pattern>/tag/search</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BubbleDataImageServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.bubbledata.BubbleDataImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BubbleDataImageServlet</servlet-name>
  	<url-pattern>/image</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>UserInfoImageServlet</servlet-name>
  	<servlet-class>com.kiwi.bubble.appengine.server.userinfo.UserInfoImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserInfoImageServlet</servlet-name>
  	<url-pattern>/userimage</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>